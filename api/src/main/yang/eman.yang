 module eman {
    yang-version 1;
    namespace "urn:opendaylight:eman";
    prefix eman;

    organization "Pajarito Technologies LLC";
    contact "Frank Sandoval francisrsandoval@gmail.com";
    description "Defines Information Model of Energy Management module.";

    revision "2017-01-05" {
        description "Initial revision of eman model";
    }

    /* Very simple API, just set and get. User passes in string indicating resource in
    Information Model
    */
    rpc get-eoAttribute {
        input {
            leaf deviceIP {
                type string;
            }
            leaf attribute {
                type string;
            }
        }
        output {
            leaf response {
                type string;
            }
        }
    }

    rpc set-eoAttribute {
        input {
            leaf deviceIP {
                type string;
            }
            leaf attribute {
                type string;
            }
           leaf value {
                type string;
            }
        }
        output {
            leaf response {
                type string;
            }
        }
    }
}
